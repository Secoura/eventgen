{
  "id": "eventgen",
  "name": "eventgen",
  "description": "Event generator for Discover",
  "version": "0.4.0",
  "provides": [
    {
      "type": "collector",
      "name": "eventgen-collector",
      "conditions": [
        {
          "match": {
            "os": "linux",
            "arch": "x64"
          },
          "path": "./bin/eventgen-linux-x64",
          "args": "generate"
        },
        {
          "match": {
            "os": "macos",
            "arch": "x64"
          },
          "path": "./bin/eventgen-macos-x64",
          "args": "generate"
        },
        {
          "match": {
            "os": "windows",
            "arch": "x64"
          },
          "path": "./bin/eventgen-windows-x64.exe",
          "args": "generate"
        }
      ],
      "source_name": "eventgen",
      "line_breaker": "\n",
      "interval": 3600000
    }
  ]
}
